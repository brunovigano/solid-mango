{
  "name": "solid-mango",
  "version": "2.5.3",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "tsc --watch",
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 -nolazy ./dist/main/server.js",
    "down": "docker-compose down",
    "up": "yarn build && docker-compose up",
    "start:dev": "ts-node-dev -r tsconfig-paths/register src/main/server.ts",
    "test": "yarn jest --clearCache && jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "prepare": "npx husky install",
    "pre-commit-lint": "npx lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.8",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.1.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "5.5.3",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "lint-staged": "^12.3.3",
    "mockdate": "^3.0.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.5.5",
    "yarn-upgrade-all": "^0.6.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "3.5.1",
    "nodemon": "^2.0.15",
    "swagger-ui-express": "4.1.4",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "17.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
